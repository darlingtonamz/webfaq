<!DOCTYPE html>
<html>
<head>
  <title>WebFAQ</title>
  <%= stylesheet_link_tag    'application', 'elements',media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'my_js', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
</head>
<body>
<% flash.each do |name, msg| %>
<%= content_tag(:div, msg, class: "alert alert-#{name}")%>
<% end%>

<%= yield %>

</body>
</html>
