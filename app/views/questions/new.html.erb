<h1>Questions#new</h1>
<p>Find me in app/views/questions/new.html.erb</p>

<%= form_for :question, url: {action: 'create'} do |f| %>
  <%= f.label :Question %>:
  <%= f.text_field :content %><br />

  <%= f.label :Answer %>:
  <%= f.text_field :answer %><br />

  <%= f.submit 'Create QA' %>
<% end %>