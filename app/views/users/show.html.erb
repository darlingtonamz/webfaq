<h1>Users#show</h1>
<p>Find me in app/views/users/show.html.erb</p>
Welcome <%= @user.name.capitalize %>

<%= link_to 'Edit', edit_user_path(@user) %><br>
<br>
<h4>FAQs for <%= @user.name.capitalize %></h4>
<table>
	<tr>
		<th>Title</th>
		<th>Created</th>
		<th>...</th>
	</tr>
	<% @faqs.each do |f| %>
		<tr>
			<td><%= f.title %></td>
			<td><%= f.created_at %></td>
			<td><%= link_to 'Edit', faq_path(f) %></td>
		</tr>
	<% end %>
</table>
