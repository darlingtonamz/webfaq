<tr id='edit_faq_<%= @faq.id%>'>
	<td><%= link_to @faq.title, user_faq_questions_path(@faq.user_id, @faq) %></td>
	<td><%= @faq.created_at %></td>
	<td><%= link_to image_tag('edit.png', alt:'Edit'), edit_user_faq_path(@faq.user_id, @faq) %></td>
	<td><%= link_to image_tag('delete.png', alt:'Delete'), user_faq_path(@faq.user_id, @faq), method: :delete %></td>
</tr>